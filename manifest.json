{
  "manifest_version": 2,
  "name": "オープンレックキャプチャー",
  "description": "openrecで映像をキャプチャーして保存する",
  "version": "1.4",

  "content_scripts": [
      {
          "matches":  ["https://www.openrec.tv/live/*"],
          "js": ["jquery-3.2.1.min.js","contentscript.js"],
          "css":["style.css"]
      }
  ],

  "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon2.png"

  }
}